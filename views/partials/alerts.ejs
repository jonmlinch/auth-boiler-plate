<% if(alerts.success){ %>
	<% alerts.success.forEach(function(msg){%>
		<div class="row">
			<div class="card green">
				<%= msg %>
			</div>
		</div>
	<%});%>
<% } %> 

<% if(alerts.error){ %>
	<% alerts.error.forEach(function(msg){%>
		<div class="row">
			<div class="card red">
				<%= msg %>
			</div>
		</div>
	<%});%>
<% } %>